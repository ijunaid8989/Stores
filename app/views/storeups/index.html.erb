<%= form_for @store , :html => {:id => "level_forms"}  do |f| %>
	<div class="form-group">
		<%= f.label :storename, "Store Name :" %>
		<%= f.text_field :store_name , :class => "form-control" , :placeholder => "Store Name" %>
	</div>
	<div class="form-group">
		<%= f.label :licence, "Licence No :" %>
		<%= f.text_field :licence , :class => "form-control" , :placeholder => "Licence" %>
	</div>
	<div class="form-group">
		<%= f.label :owner_name, " Owner Name :" %>
		<%= f.text_field :owner_name , :class => "form-control" , :placeholder => "Owner Name" %>
	</div>
	<div class="form-group">
		<%= f.label :owner_cnic, "Owner CNIC :" %>
		<%= f.text_field :owner_cnic , :class => "form-control" , :placeholder => "Owner Cnic" %>
	</div>
		<div class="form-group">
		<%= f.label :owner_email, "Owner Email :" %>
		<%= f.text_field :email , :class => "form-control" , :placeholder => "Owner Email" %>
	</div>
		<div class="form-group">
		<%= f.label :password, "Account Password :" %>
		<%= f.password_field :password , :class => "form-control" , :placeholder => "Account Password" %>
	</div>
	<%= f.submit :SignUp , :class => "btn btn-primary" %>
<% end %>